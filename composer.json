{
    "name": "bskscmn/laravel-full-calendar",
    "description": "This is a fork of https://github.com/nelkasovic/laravel-full-calendar.",
    "authors": [
        {
            "name": "Qlick by Nermin Elkasovic",
            "email": "nermin@qlick.ch",
            "homepage": "https://qlick.ch",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "illuminate/database": "^7.0|^8.0|^9.0|10.0",
        "illuminate/support": "^7.0|^8.0|^9.0|10.0",
        "laravel/helpers": "^1.0"
    },
    "license": "MIT",
    "homepage": "https://github.com/bskscmn/laravel-full-calendar",
    "support": {
        "issues": "https://github.com/bskscmn/laravel-full-calendar/issues",
        "wiki": "https://github.com/bskscmn/laravel-full-calendar/wiki",
        "source": "https://github.com/bskscmn/laravel-full-calendar",
        "docs": "https://github.com/bskscmn/laravel-full-calendar"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "LaravelFullCalendar\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelFullCalendar\\FullCalendarServiceProvider"
            ],
            "aliases": {
                "Calendar": "LaravelFullCalendar\\Facades\\Calendar"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
